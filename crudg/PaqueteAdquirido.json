{
    "model_name":"PaqueteAdquirido",
    "pagination":20,
    "soft_deletes":"yes",
    "pk":"id",
    "localize":"yes",
    "table":"PaquetesAdquiridos",
    "fields": [
        {
            "name": "fechaInicio",
            "type": "date"
        },
        {
            "name": "fechaFin",
            "type": "date"
        },
        {
            "name": "precio",
            "type": "string"
        },
        {
            "name": "turista_id",
            "type": "integer#unsigned"
        },
        {
            "name": "paquete_id",
            "type": "integer#unsigned"
        }
    ],

    "foreign_keys": [
        {
            "column": "turista_id",
            "references": "id",
            "on": "Turistas",
            "onDelete": "cascade"
        },

        {
            "column": "paquete_id",
            "references": "id",
            "on": "Paquetes",
            "onDelete": "cascade"
        }

        ],
    "relationships": [
        {
            "name": "Turistas",
            "type": "belongsTo",
            "class": "App\\Turista"
        },
        {
            "name": "Paquetes",
            "type": "belongsTo",
            "class": "App\\Paquete"
        },
        {
            "name": "DispositivosAsignados",
            "type": "hasMany",
            "class": "App\\DispositivosAsignado"
        }
    ],
    "validations": [
        {
            "field": "precio",
            "rules": "required|min:2|max:20"
        }

    ]
	
}