{
	"model_name":"Turista",	
	"pagination":50,
	"soft_deletes":"yes",
	"pk":"id",
	"localize":"yes",
	"table":"Turistas",
    "fields": [		
        {
            "name": "nombre",
            "type": "string"
 
        }
        ,
        {
            "name": "apellido",
            "type": "string"
        }
        ,
        {
            "name": "telefono",
            "type": "string"
        }
        ,
        {
            "name": "email",
            "type": "string"
        },
        {
            "name": "Usuario_id",
             "type": "integer#unsigned"
        }
        ],
    "foreign_keys": [ 
    	{
            "column": "Usuario_id",
            "references": "id",
            "on": "Usuarios",
            "onDelete": "cascade"
        }
    ],
        "relationships": [
        {
            "name": "PaquetesAdquiridos",
            "type": "belongsTo",
            "class": "App\\PaqueteAdquirido"
        },
        {
            "name": "Usuarios",
            "type": "belongsTo",
            "class": "App\\Usuario"
        }
    ],

    "validations": [
        {
            "field": "nombre",
            "rules": "required|min:3"
        },
        {
            "field": "apellido",
            "rules": "required|min:10"
        },
		{
            "field": "email",
            "rules": "required|min:10|max:50"
        }
    ]
	
}