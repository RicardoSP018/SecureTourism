{
	"model_name":"Producto",	
	"pagination":50,
	"soft_deletes":"yes",
	"pk":"id",
	"localize":"yes",
	"table":"productos",
    "fields": [		
        {
            "name": "Modelo",
            "type": "string"
 
        }
        ,
        {
            "name": "Cantidad",
            "type": "string"
        },
        {
            "name": "Descripcion",
            "type": "string"
        }
        ,
        {
            "name": "Precio",
            "type": "string"
        },
        {
            "name": "Imagen",
            "type": "file"
        },
        {
            "name": "proveedor_id",
             "type": "integer#unsigned"
        }
        ],
     "foreign_keys": [
        {
            "column": "proveedor_id",
            "references": "id",
            "on": "proveedores",
            "onDelete": "cascade"
        }
    ],
    "relationships": [
        {
            "name": "proveedores",
            "type": "belongsTo",
            "class": "App\\Proveedor"
        }
    ],

    "validations": [
        {
            "field": "Descripcion",
            "rules": "required|min:8"
        }
    ]
	
}