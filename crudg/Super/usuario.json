{
	"model_name":"Usuario",	
	"pagination":50,
	"soft_deletes":"yes",
	"pk":"id",
	"localize":"yes",
	"table":"Usuarios",
    "fields": [		
        {
            "name": "Username",
            "type": "string"
 
        }
        ,
        {
            "name": "Correo",
            "type": "email"
        }
        ,
        {
            "name": "Contrasena",
            "type": "password"
        }

        ],
    "relationships": [
        {
            "name": "agencias",
            "type": "hasOne",
            "class": "App\\Agencia"
        }
    ],

    "validations": [
        {
            "field": "Username",
            "rules": "required|min:3|max:50"
        },
		{
            "field": "Correo",
            "rules": "required|min:5"
        },
        {
            "field": "Contrasena",
            "rules": "required|min:8"
        }
    ]
	
}