{
	"model_name":"Administrador",	
	"pagination":50,
	"soft_deletes":"yes",
	"pk":"id",
	"localize":"yes",
	"table":"Administradores",
    "fields": [		
        {
            "name": "nombre",
            "type": "string"
 
        }
        ,
        {
            "name": "correo",
            "type": "email"
        }
        ,
        {
            "name": "contrasena",
            "type": "password"
        },
        {
            "name": "telefono",
            "type": "password"
        },
        
        {
            "name": "usuario_id",
             "type": "integer#unsigned"
        }

        ],
    "relationships": [
        {
            "name": "Usuarios",
            "type": "belongsTo",
            "class": "App\\Usuario"
        },
         {
            "name": "Agencias",
            "type": "hasOne",
            "class": "App\\Agencia"
        }
    ],
    "foreign_keys": [
        {
            "column": "usuario_id",
            "references": "id",
            "on": "Usuarios",
            "onDelete": "cascade"
        }
        ],

    "validations": [
		{
            "field": "correo",
            "rules": "required|min:5"
        },
        {
            "field": "contrasena",
            "rules": "required|min:8"
        }
    ]
	
}