{
	"model_name":"Suscripcion",
	"soft_deletes":"yes",
	"pk":"id",
	"localize":"yes",
	"table":"Suscripciones",
    "fields": [	
        {
            "name": "nombre",
            "type": "string"
        },
        {
            "name": "descripcion",
            "type": "string"
        },	
        {
            "name": "estado",
            "type": "string"
        },
        {
            "name": "inicio",
            "type": "date"
        },
        {
            "name": "vencimiento",
            "type": "date"
        }, 
         {
            "name": "agencia_id",
             "type": "integer#unsigned"
        }
 
    ],
     "relationships": [
        {
            "name": "Servicios",
            "type": "hasMany",
            "class": "App\\Servicio"
        },
        {
            "name": "Agencias",
            "type": "belongsTo",
            "class": "App\\Agencia"
        }
    ],
     "foreign_keys": [
        {
            "column": "agencia_id",
            "references": "id",
            "on": "Agencias",
            "onDelete": "cascade"
        }
        ],

    "validations": [
        {
            "field": "estado",
            "rules": "required|min:5|max:20"
        }
    ]
}